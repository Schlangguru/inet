<?xml version="1.0" encoding="UTF-8"?>
<scave:Analysis xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:scave="http://www.omnetpp.org/omnetpp/scave">
  <inputs>
    <inputs name="/inet/samples/ratecontrol/results/General-*.vec"/>
    <inputs name="/inet/samples/ratecontrol/results/General-*.sca"/>
  </inputs>
  <datasets>
    <datasets name="AARFRateControl">
      <items xsi:type="scave:Add" filterPattern="name(databitrate:vector) AND file(/inet/samples/ratecontrol/results/General-1.vec) AND module(*.Host[*].*)  " type="VECTOR"/>
      <items xsi:type="scave:Add" filterPattern="name(throughput:vector) AND file(/inet/samples/ratecontrol/results/General-1.vec)  " type="VECTOR"/>
      <items xsi:type="scave:LineChart" name="AARFRateControl" lineNameFormat="">
        <properties name="Graph.Title" value="AARFRateControl"/>
        <properties name="Y.Axis.Max" value="7.0E7"/>
        <properties name="X.Axis.Max" value="25.0"/>
        <properties name="Line.Type/RateControlExample.Host[0].wlan[0].mac.rateControl databitrate:vector" value="SampleHold"/>
        <properties name="Line.Type/RateControlExample.sinkHost.udpApp[0] throughput:vector" value="Linear"/>
        <properties name="Symbols.Type" value="Dot"/>
        <properties name="Symbols.Size" value="2"/>
      </items>
    </datasets>
    <datasets name="No rate control">
      <items xsi:type="scave:Add" filterPattern="name(throughput:vector) AND file(/inet/samples/ratecontrol/results/General-0.vec) " type="VECTOR"/>
      <items xsi:type="scave:LineChart" name="No rate control" lineNameFormat="">
        <properties name="Graph.Title" value="No rate control"/>
        <properties name="Y.Axis.Max" value="7.0E7"/>
        <properties name="X.Axis.Max" value="25.0"/>
        <properties name="Symbols.Type" value="Dot"/>
        <properties name="Symbols.Size" value="2"/>
        <properties name="Line.Type" value="Linear"/>
        <properties name="Line.Color" value="red"/>
      </items>
    </datasets>
    <datasets name="Throughput">
      <items xsi:type="scave:Add" filterPattern="name(throughput:vector)" type="VECTOR"/>
      <items xsi:type="scave:LineChart" name="Throughput" lineNameFormat="">
        <properties name="Graph.Title" value="Throughput"/>
        <properties name="Y.Axis.Max" value="7.0E7"/>
        <properties name="X.Axis.Max" value="25.0"/>
        <properties name="Symbols.Type" value="Dot"/>
        <properties name="Symbols.Size" value="2"/>
        <properties name="Line.Type" value="Linear"/>
      </items>
    </datasets>
    <datasets name="Databitrate">
      <items xsi:type="scave:Add" filterPattern="name(databitrate:vector) AND module(*.Host[*].*) " type="VECTOR"/>
      <items xsi:type="scave:LineChart" name="Bitrate" lineNameFormat="">
        <properties name="Graph.Title" value="Bitrate"/>
        <properties name="Y.Axis.Max" value="7.0E7"/>
        <properties name="X.Axis.Max" value="25.0"/>
        <properties name="Symbols.Type" value="Dot"/>
        <properties name="Symbols.Size" value="2"/>
        <properties name="Line.Type" value="SampleHold"/>
      </items>
    </datasets>
  </datasets>
  <chartSheets>
    <chartSheets name="default" charts="//@datasets/@datasets.0/@items.2 //@datasets/@datasets.1/@items.1 //@datasets/@datasets.2/@items.1 //@datasets/@datasets.3/@items.1"/>
    <chartSheets name="Throughput vs databitrate" charts="//@datasets/@datasets.0/@items.2 //@datasets/@datasets.1/@items.1"/>
  </chartSheets>
</scave:Analysis>
